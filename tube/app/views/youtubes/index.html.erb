<p id="notice"><%= notice %></p>

<h1>Youtubes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Video file</th>
      <th>Thumbnail</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @youtubes.each do |youtube| %>
      <tr>
        <td><%= youtube.title %></td>
        <td><%= youtube.video_file %></td>
        <td><%= youtube.thumbnail %></td>
        <td><%= youtube.description %></td>
        <td><%= link_to 'Show', youtube %></td>
        <td><%= link_to 'Edit', edit_youtube_path(youtube) %></td>
        <td><%= link_to 'Destroy', youtube, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Youtube', new_youtube_path %>
